<div class="dialog" style="margin-top: 62px;">
  <div class="dialog--item" style="margin-top: 20px; width: 100%" @click="alert">
    <btn width="100%">Open a Alert Dialog</btn>
  </div>

  <div class="dialog--item" style="margin-top: 20px; width: 100%" @click="confirm">
    <btn width="100%">Open a Confirm Dialog</btn>
  </div>

  <div class="dialog--item" style="margin-top: 20px; width: 100%" @click="prompt">
    <btn width="100%">Open a Prompt Dialog</btn>
  </div>
</div>
