<div class="dialog" style="margin-top: 62px;">
  <div class="dialog--item" style="margin: 20px auto 0; width: 90%" @click="alert">
    <btn width="100%">Open a Alert Dialog</btn>
  </div>

  <div class="dialog--item" style="margin: 20px auto 0; width: 90%" @click="confirm">
    <btn width="100%">Open a Confirm Dialog</btn>
  </div>

  <div class="dialog--item" style="margin: 20px auto 0; width: 90%" @click="prompt">
    <btn width="100%">Open a Prompt Dialog</btn>
  </div>
</div>
